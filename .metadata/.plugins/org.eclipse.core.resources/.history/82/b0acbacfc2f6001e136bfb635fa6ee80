package static_initializer_block;

import java.util.Scanner;

public class Main {
	
	static {
		
	}

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		try {
			int b = scan.nextInt();
			int h = scan.nextInt();
			if(b <= 0 || h <= 0)
		}
		catch(Exception e) {
			System.out.println("java.lang.Exception: Breadth and height must be positive");
		}
	}

}
